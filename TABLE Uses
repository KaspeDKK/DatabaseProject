CREATE TABLE Uses
	(
	RepairDate date,
	BikeSN VARCHAR(20),
	PartsSN VARCHAR(20),
  PRIMARY KEY (PartsSN, BikeSN, RepairDate), 
	FOREIGN KEY(RepairDate) REFERENCES RepairJobs(RepairDate) ON DELETE CASCADE,
	FOREIGN KEY(BikeSN) REFERENCES Bikes(BikeSN) ON DELETE CASCADE,
  FOREIGN KEY(PartsSN) REFERENCES Parts(PartsSN) ON DELETE CASCADE
	);
